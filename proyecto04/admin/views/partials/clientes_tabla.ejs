<main class="col-md-8 ms-sm-auto col-lg-10 px-md-4">
    <div class="table-responsive">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-5">
                        <h2>Customers</h2>
                    </div>
                    <div class="col-sm-7">
                        <a href="#" class="btn btn-secondary"><i class="material-icons">&#xE147;</i> <span>Add New Photo</span></a>
                        <a href="#" class="btn btn-secondary"><i class="material-icons">&#xE24D;</i> <span>Export to Excel</span></a>                       
                    </div>
                </div>
            </div>
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Customer name</th>
                        <th>First name</th>
                        <th>Last name</th>
                        <th>Phone</th>
                        <th>country</th>
                    </tr>
                </thead>
                <tbody>
                    <% arrClientes.forEach((cliente)=> { %>
                    <tr>
                        <td><%= cliente.customerNumber %></td>
                        <td><%= cliente.customerName %></td>
                        <td><%= cliente.contactFirstName %></td>
                        <td><%= cliente.contactLastName %></td>
                        <td><%= cliente.phone %></td>
                        <td><%= cliente.country %></td>
                        <td>
                            <a href="/customers/orders/<%= cliente.customerNumber %>" class="settings" title="Settings" data-toggle="tooltip"><i class="material-icons">&#10148;</i></a>
                        </td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
            <div class="clearfix">
                <div class="hint-text">Showing <b>5</b> out of <b>25</b> entries</div>
                <ul class="pagination">
                    <li class="page-item disabled"><a href="#">Previous</a></li>
                    <li class="page-item"><a href="#" class="page-link">1</a></li>
                    <li class="page-item"><a href="#" class="page-link">2</a></li>
                    <li class="page-item active"><a href="#" class="page-link">3</a></li>
                    <li class="page-item"><a href="#" class="page-link">4</a></li>
                    <li class="page-item"><a href="#" class="page-link">5</a></li>
                    <li class="page-item"><a href="#" class="page-link">Next</a></li>
                </ul>
            </div>
        </div>
    </div>  
</main>